<% include ./partials/header.ejs %>
<div class="homecard">
<h1>Blog Posts by <%= author %></h1>
<p>This is for the reader only </p>

<% posts.forEach((post) => { %>
  <div class="homecardblog">
    <h2><%= post.title %></h2>
    <p><%= post.body %></p>
    <p><%= post.id %></p>
    <p><em>Author: <%= post.author %></em></p>
    
  </div>
<% }) %>
</div>
<% include ./partials/footer.ejs %>