<% include ./partials/header.ejs %>
<div class="homecard">
<h1>Blog Posts by <%= author %></h1>
<p>This is the route where from the dashboard user is gting</p>

<% posts.forEach((post) => { %>
  <div class="homecardblog">
    <h2><%= post.title %></h2>
    <p><%= post.body %></p>
    <p><%= post.id %></p>
    <p><em>Author: <%= post.author %></em></p>
    <a href="/posts/<%=post.id%>">Delete</a>
    <a href="/blog/<%=post.id%>">Edit</a>
    
  </div>
<% }) %>
</div>
<% include ./partials/footer.ejs %>